import java.util.*;
public class Alice
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int sum=0;
        int temp;
        while(n>0){
        temp=n%10;
        sum=sum+temp;
        n=n/10;
        }
        System.out.println(sum);
        
        
    }
}
