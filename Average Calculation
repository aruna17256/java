import java.util.*;
public class Avg
{
    public static void main(String args[])
    {
        Scanner sc = new Scanner(System.in);
        float a = sc.nextInt();
        float b = sc.nextInt();
        float c = sc.nextInt();
        float d = sc.nextInt();
        float e = sc.nextInt();
        float p= a+b+c+d+e;
        System.out.println(p/5);
     
    }
}
