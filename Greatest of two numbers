import java.util.*;
public class Compare
{
    public static void main(String args[])
    {
        int a;
        int b;
        Scanner sc = new Scanner(System.in);
        a = sc.nextInt();
        b = sc.nextInt();
        if(a>b){
            System.out.println(a + " is greater than " + b);
        }else if(a==b)
        {
            System.out.println(a + " is equal to " + b);
            
        }
        else{
            System.out.println(a + " is less than " + b);
            
        }
    }
}
